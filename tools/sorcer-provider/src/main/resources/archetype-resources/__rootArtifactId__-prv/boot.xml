<?xml version="1.0"?>
<!-- 
Replace "provider.name" with your provider name and specify property value for "provider.class."
Also you might need to adjust the classpath (provider.classpath) for your provider 
-->

<project name="${providerInterface} Provider" default="boot.provider" basedir=".">
	
	<property name="pom.file" value="${basedir}/pom.xml" />
	<property environment="env" />
	<property name="provider.config" value="${basedir}/target/classes/config/services.config" />
    <property name="provider.debug" value="false" />
	<import file="${env.SORCER_HOME}/bin/common-run.xml" />
		
</project>

